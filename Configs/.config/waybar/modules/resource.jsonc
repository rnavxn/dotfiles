"wlr/taskbar": {
    "all-outputs": true,
    "active-first": true,
    "markup": true,
    "format": "{icon}",
    "rotate": "0",
    "icon-size": 26,
    "icon-theme": "Tela-nord-dark",
    "spacing": 0,
    "tooltip-format": "{title}{app_id}",
    "on-click": "activate",
    "on-click-right": "fullscreen",
    "on-click-middle": "close",
    "ignore-list": [
